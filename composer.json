{
    "name": "hautelook/alice-bundle",
    "description": "Symfony bundle to manage fixtures with Alice and Faker.",
    "keywords": ["Symfony", "Fixture", "ORM", "Alice", "Faker"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Baldur Rensch",
            "email": "brensch@gmail.com"
        },
        {
            "name": "ThÃ©o FIDRY",
            "email": "theo.fidry@gmail.com",
            "homepage": "https://github.com/theofidry"
        }
    ],

    "autoload": {
        "psr-4": {
            "Hautelook\\AliceBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hautelook\\AliceBundle\\": [
                "fixtures",
                "tests"
            ]
        }
    },

    "require": {
        "php": ">=8.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/data-fixtures": "^1.5",
        "doctrine/orm": "^2.8",
        "doctrine/persistence": "^2.1",
        "psr/log": "^1.0",
        "symfony/finder": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "theofidry/alice-data-fixtures": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpspec/prophecy": "^1.7",
        "symfony/phpunit-bridge": "^5.1"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev",
            "dev-pifid": "dev-pifid-1.0"
        },
        "bin-dir": "bin",
        "sort-packages": true
    }
}
